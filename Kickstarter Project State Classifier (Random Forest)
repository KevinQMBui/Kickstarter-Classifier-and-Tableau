   ],
   "source": [
    "# Mean imputation to fill remaining missing values for numerical columns\n",
    "fs['goal'] = fs['goal'].fillna(value=fs['goal'].mean())\n",
    "fs['pledged'] = fs['pledged'].fillna(value=fs['pledged'].mean())\n",
    "fs['backers'] = fs['backers'].fillna(value=fs['backers'].mean())\n",
    "fs['usd pledged'] = fs['usd pledged'].fillna(value=fs['usd pledged'].mean())\n",
    "fs.isnull().sum()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Turning 'backers' to int because that column being float makes no sense\n",
    "fs['backers'] = fs['backers'].astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Kickstarter's income, which is 5% of successful project pledges\n",
    "fs['revenue'] = fs[fs['state']=='successful']['pledged']*.05\n",
    "fs['revenue'].fillna(value=0,inplace=True)"
   ]
  },
